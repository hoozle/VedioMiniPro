.icon{
  width:54rpx;
  height: 54rpx;
}
.tabbar{
  width:100%;
  position: fixed;
  bottom:0;
  padding:10rpx;
  margin-left:-4rpx;
  background:#F7F7FA;
  font-size:24rpx;
  color:#8A8A8A;
  box-shadow: 3rpx 3rpx 3rpx 3rpx #aaa; 
  z-index: 9999;
}

 .tabbar-item{
  float:left;
  width:33.3%;
  text-align: center;
  overflow: hidden;
}
/*当前字体颜色*/
.tabbartext{
  color:blue;
}
.navigator-hover{
  background-color: rgba(0, 0, 0, 0);
}